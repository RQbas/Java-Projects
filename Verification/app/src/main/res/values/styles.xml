<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/grayDark</item>
        <item name="android:textColorPrimary">@color/purpleDark</item>
        <item name="android:textColor">@color/purpleDark</item>
        <item name="colorPrimaryDark">@color/purpleDark</item>
        <item name="colorAccent">@color/grayLight</item>
    </style>


</resources>